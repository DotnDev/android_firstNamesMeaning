<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
<item>

<shape android:shape="rectangle">
    <corners android:radius="3dp" />

    <solid android:color="@color/white" />

    <padding
        android:left="0dp"
        android:top="0dp"
        android:right="0dp"
        android:bottom="0dp" />
